<!--地理拓扑单元选择
	[URBAN]：城市场景
	[HIGHSPEED]：高速场景-->	
<GTTMode>URBAN</GTTMode>


<!--无限资源管理模式选择
	[TDM_DRA]：簇间时分的竞争式资源管理模式
	[ICC_DRA]：簇间并发的竞争式资源管理模式
	[RR]：集中式轮询调度模式-->
<RRMMode>TDM_DRA</RRMMode>


<!--无限资源管理模式选择
	[SINR_MRC]：SINR_MRC
	[SINR_MMSE]：SINR_MMSE-->
<WTMode>SINR_MRC</WTMode>


<!--仿真总时间，单位TTI(毫秒)-->
<NTTI>100</NTTI>


<!--线程数量-->
<ThreadNum>4</ThreadNum>


<!--地理位置刷新周期，单位TTI-->
<locationUpdateNTTI>100</locationUpdateNTTI>











